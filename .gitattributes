# Model files
*.pth filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.pt filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.safetensors filter=lfs diff=lfs merge=lfs -text
# Compiled binaries
*.so filter=lfs diff=lfs merge=lfs -text
*.o filter=lfs diff=lfs merge=lfs -text
*.a filter=lfs diff=lfs merge=lfs -text
*.egg filter=lfs diff=lfs merge=lfs -text
# Archives
*.gz filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
# Detectron2 directory - use multiple patterns
detectron2/** filter=lfs diff=lfs merge=lfs -text
detectron2/ filter=lfs diff=lfs merge=lfs -text
detectron2/* filter=lfs diff=lfs merge=lfs -text
oneformer/**/*.pth filter=lfs diff=lfs merge=lfs -text
oneformer/**/*.pkl filter=lfs diff=lfs merge=lfs -text
oneformer/**/*.pt filter=lfs diff=lfs merge=lfs -text
oneformer/**/*.bin filter=lfs diff=lfs merge=lfs -text
